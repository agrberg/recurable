# frozen_string_literal: true

source 'https://rubygems.org'

gem 'activemodel', '~> 8.0.0'
gem 'activerecord', '~> 8.0.0'
gem 'activesupport', '~> 8.0.0'

gemspec path: '..'

group :development, :test do
  gem 'rake'
  gem 'rspec'
  gem 'rubocop'
  gem 'rubocop-performance'
  gem 'rubocop-rails'
  gem 'rubocop-rake'
  gem 'rubocop-rspec'
  gem 'simplecov'
end
